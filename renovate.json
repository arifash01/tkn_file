{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["(?P<filename>.+)\\.yaml$"],
      "matchStrings": [
        "image:\\s+(?<depName>(?:[^/]+/)*)(?<currentValue>[^\\s]+)"
      ],
      "autoReplaceStringTemplate": "image: {{{depName}}}:{{{newValue}}}",
      "registryUrlTemplate": "https://pantheon.corp.google.com/gcr/images/distroless/global/base-debian12?e=13802955&mods=logs_tg_prod"
    }
  ],
  "kubernetes": {
    "fileMatch": ["(?P<filename>.+)\\.yaml$"]
  }

}